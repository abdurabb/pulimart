<body class="handheld-toolbar-enabled">
  <!-- Google Tag Manager (noscript)-->

  <!-- Sign in / sign up modal-->
  <main class="page-wrapper">
    <!-- Navbar 3 Level (Light)-->

    <!-- Page Title-->
    <div class="page-title-overlap pt-3" style="background-color: #20547d;">
      <div class="container d-lg-flex justify-content-between ">
       <a href="/"> <div class="order-lg-2 mb-2 mb-lg-0 ">
          <img src="landsheetImages/logo.png" alt="" width="90px" height="80px">
          <h3 class=" " style="color: crimson;">Pulimart</h3>
        </div></a>
        <div class="order-lg-1 pe-lg-4 text-center text-lg-start">
          <h1 class="h3 text-light mb-0">Your Favourite </h1>
        </div>
      </div>
    </div>


    <div class="container pb-5 mb-2 mb-md-4">
      <div class="row">
        <!-- List of items-->

        <section class="col-12">
          <div class="d-flex justify-content-between align-items-center pt-3 pb-4 pb-sm-5 mt-1">
            <h2 class="h6 text-light mb-0">Products</h2>
          </div>
          <!-- Item-->
          <%if(wishListProducts.length>0){%>
               
            <table class="table  text-center">
                <thead class="thead-dark ">
                  <tr>
                    <th scope="col">Sl-No</th>
                    <th scope="col">Product</th>
                    <th scope="col">Name</th>
                    <th scope="col">Price</th>
                    <th scope="col">Action</th>
                    <th scope="col">Move to Cart</th>

                  </tr>
                </thead>
                <% for(let i=0;i<wishListProducts.length;i++){ %>
                <tbody>
                  <tr>
                    <th scope="row" class="pt-5"><%=i+1%></th>
                    <td class=""> <img width="80px" height="" src="/productimages/<%=products[i].image[0]%>" alt=""> </td>
                    <td class="pt-5"><%=wishList.product[i].name%></td>
                    <td class="pt-5"><%=wishList.product[i].price%></td>
                    
                    <td class="pt-5"><a href="/removeWishList?id=<%=products[i]._id%>">
                        <button class="btn-outline-danger rounded-pill">Remove</button> </a>
                    </td>
                    <td class="pt-5">
                      <a href="/addToCart?id=<%=products[i]._id%>">
                        <button class="btn-outline-success rounded-pill">Add to Cart</button> </a>
                  </tr>

                  <%}%>
                </tbody>
               
              </table>
                <%} else {%>
                  <div class="text-center">
                    <h1 class="text-danger"> Your WishList is Empty</h1>
                    <a href="/shope"><button class="btn btn-outline-success">Please Add The Product</button></a>
                  </div>
                  <%}%>
                   
                    <!-- item -->
        </section>
        <!-- Sidebar-->
        
      </div>
    </div>
  </main>
  <!-- Footer-->

  


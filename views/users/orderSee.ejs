<!-- Body-->

<body class="handheld-toolbar-enabled">
    <!-- Google Tag Manager (noscript)-->

    <!-- Sign in / sign up modal-->
    <main class="page-wrapper">
        <!-- Navbar 3 Level (Light)-->

        <!-- Page Title-->
        <div class="page-title-overlap pt-3" style="background-color: #20547d;">
            <div class="container d-lg-flex justify-content-between ">
                <a href="/">
                    <div class="order-lg-2 mb-2 mb-lg-0 ">
                        <img src="landsheetImages/logo.png" alt="" width="90px" height="80px">
                        <h3 class=" " style="color: crimson;">Pulimart</h3>
                    </div>
                </a>
                <div class="order-lg-1 pe-lg-4 text-center text-lg-start">
                    <h1 class="h3 text-light mb-0">Your Orders</h1>
                </div>

            </div>
        </div>

       

                <!-- List of items-->


                <%if(order.length>0) {%>
                    <!-- ----------- -->
                    <div class="table-responsive mt-3"  data-simplebar>
                    <table class="table col-12 ">
                        <thead class="  text-center " style="background-color: #20547d; ">
                            <tr>

                                <th scope="col" class="pl-5 ">
                                    <p class="text-white "> Order Id</p>
                                </th>
                                <th scope="col" class="pl-5">
                                    <p class="text-white"> Customer Name </p>
                                </th>
                                <th scope="col" class="pl-5">
                                    <p class="text-white"> Address </p>
                                </th>
                                <th scope="col" class="pl-5">
                                    <p class="text-white"> Payment</p>
                                </th>
                                <th scope="col" class="pl-5">
                                    <p class="text-white"> Date</p>
                                </th>
                                <th scope="col" class="pl-5">
                                    <p class="text-white">Price</p>
                                </th>
                                <th scope="col" class="pl-5">
                                    <p class="text-white"> Coupon Discount</p>
                                </th>
                                <th scope="col" class="pl-5">
                                    <p class="text-white"> SubTotal</p>
                                </th>

                                <th scope="col" class="pl-5">
                                    <p class="text-white"> Status</p>
                                </th>
                                <th scope="col" class="pl-5">
                                    <p class="text-white"> Track Order</p>
                                </th>
                            </tr>
                        </thead>
                        <!-- <tbody class="text-center "> -->
                        <tbody class="text-center  ">
                            <%for(let i of order){%>

                                <tr class="table-success">


                                    <td class="pl-5">
                                        <%=i._id%>
                                    </td>
                                    <td class="pl-5">
                                        <%=i.customer.name%>
                                    </td>
                                    <td class="pl-5">
                                        <%=i.customer.Address%>,<%=i.customer. postcode%>,<%=i.customer.city%>,
                                                    <%=i.customer.phone%>
                                    </td>

                                    <td class="pl-5">
                                        <%=i.paymentMethod%>
                                    </td>
                                    <td class="pl-5">
                                        <%=i.createdDate.toLocaleDateString()%>
                                    </td>
                                    <td class="pl-5">
                                        <%=i.totalPrice%>
                                    </td>
                                    <td class="pl-5">
                                        <%=i.discout%>
                                    </td>
                                    <td class="pl-5">
                                        <%=i.subTotal%>
                                    </td>

                                    <td>
                                        <%= i.status%>
                                    </td>

                                    <td class="pl-5"><a href="/orderDetail?id=<%=i._id%>">
                                            <button class="rounded-pill"
                                                style="background-color: #20547d;">View</button>
                                        </a>
                                    </td>
                                </tr>

                                <%}%>
                                    <!-- </tbody> -->
                        </tbody>
                    </table>
                </div>

                    <!-- ------------- -->
                    <%} else {%>
                        Did not Orderd anything please Purchase
                        <%}%>


                            <!-- Sidebar-->

            </div>
        </div>
    </main>
    <!-- Footer-->



    <!-- Toolbar for handheld devices (Default)-->
 
    <!-- Back To Top Button--><a class="btn-scroll-top" href="#top" data-scroll><span
            class="btn-scroll-top-tooltip text-muted fs-sm me-2">Top</span><i class="btn-scroll-top-icon ci-arrow-up">
        </i></a>
    <!-- Vendor scrits: js libraries and plugins-->
    <script src="vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="vendor/simplebar/dist/simplebar.min.js"></script>
    <script src="vendor/tiny-slider/dist/min/tiny-slider.js"></script>
    <script src="vendor/smooth-scroll/dist/smooth-scroll.polyfills.min.js"></script>
    <!-- Main theme script-->
    <script src="/productCss/theme.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js"
        integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js"
        integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF"
        crossorigin="anonymous"></script>
</body>


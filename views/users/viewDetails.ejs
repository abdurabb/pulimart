          <div class="col-lg-5 pt-4 pt-lg-0">
            <div class="product-details ms-auto pb-5">
              <div class="position-relative me-n4 mb-3 mt-5">
                <div class="text-center">
                  <h6>
                    <%=dataDetail.name%>
                  </h6>
                  <h6>
                    <%=dataDetail.description%>
                  </h6>
                  <h6>Rs- <%=dataDetail.price%>
                  </h6>
                </div>
               
              </div>

              <div class="position-relative me-n4 mb-3">
                <a class="btn  btn-shadow d-block w-100 mt-4" href="/addToCart?id=<%=dataDetail._id%>"
                  style="background-color:#20547d">Add To Cart</a>
              </div>

              <div class="position-relative me-n4 mb-3">
                <a class="btn  btn-shadow d-block w-100 mt-4" href="/addToWishList?id=<%=dataDetail._id%>"
